@using KOCR_Web.Models
@*@model CWDocsIndexViewModel*@
@{
    ViewData["Title"] = "CWDocs Create User Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
    Layout = "_Layout";
}

<h1>New User</h1>
<form method="post" asp-controller="Account" asp-action="CreateUser">
    <br>
    <br>
    <div>
        <div class="row">
            <div class="col-4">
                <input name="userName" />
            </div>
        </div>
        <div class="row">
            <div class="col-4">
                <input name="password" />
            </div>
        </div>
        <div class="row">
            <div class="col-4">
                <button type="submit">Create User</button>
            </div>
        </div>
    </div>


</form>
